{
	"cli": true,
	"target": "directory",
	"adapterName": "aurora-nowcast",
	"title": "Aurora Nowcast",
	"description": "Provides current (nowcast) data on aurora (northern and southern lights) activity for a given location, based on NOAA information. This is not a forecast.",
	"keywords": [
		"noaa",
		"aurora",
		"northern lights",
		"aurora borealis",
		"aurora australis",
		"space weather",
		"nowcast"
	],
	"expert": "yes",
	"features": [
		"adapter"
	],
	"adminFeatures": [],
	"type": "weather",
	"startMode": "schedule",
	"scheduleStartOnChange": "yes",
	"connectionType": "cloud",
	"dataSource": "poll",
	"connectionIndicator": "no",
	"language": "JavaScript",
	"nodeVersion": "22",
	"adminUi": "none",
	"tools": [
		"ESLint",
		"type checking"
	],
	"eslintConfig": "official",
	"releaseScript": "yes",
	"devServer": "local",
	"indentation": "Tab",
	"quotes": "double",
	"authorName": "Christian Menne",
	"authorGithub": "chrmenne",
	"authorEmail": "publicdevelopment@christianmenne.de",
	"gitRemoteProtocol": "HTTPS",
	"gitCommit": "yes",
	"defaultBranch": "main",
	"license": "GNU GPLv3",
	"dependabot": "yes",
	"creatorVersion": "3.1.2"
}